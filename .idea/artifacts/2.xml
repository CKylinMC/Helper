<component name="ArtifactManager">
  <artifact build-on-make="true" name="构建文档包">
    <output-path>$PROJECT_DIR$/out/artifacts/</output-path>
    <properties id="ant-preprocessing">
      <options enabled="true">
        <file>file://$PROJECT_DIR$/build.xml</file>
        <target>buildDocs</target>
      </options>
    </properties>
    <root id="root">
      <element id="archive" name="Helper-docs.jar">
        <element id="dir-copy" path="$PROJECT_DIR$/docs" />
      </element>
    </root>
  </artifact>
</component>